    <body>
        <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
        <link href='https://fonts.googleapis.com/css?family=Raleway:500,400,300,200,100' rel='stylesheet' type='text/css'>
        <canvas class="container" id="container" role="main"></canvas>
        <div class="center">
            <form method="post" action="/login">
                <div class="login-block">
                    <h1>Login</h1>
                    <input type="text" value="" placeholder="Register Number" name="username" id="username" />
                    <input type="password" value="" placeholder="Password" name="password" id="password" />
                    <button type="submit">Submit</button>
                    <a href="/register">Register</a><br>
                    <%= message %>
                </div>
            </form>
        </div>
      <div class="blur blurTop"><canvas class="canvas"id="blurCanvasTop"></canvas></div>
      <div class="blur blurBottom"><canvas width="1500px" height="1500px" class="canvas" id="blurCanvasBottom"></canvas></div>
        
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
